var result = 100 + 100; //200
console.log(result);

result = "$" + 100; //$100
console.log(result);

result = +"100" * 10; //1000
console.log(result);

var x = 10;
var y = "10";

console.log(x == y); //true
console.log(x == y); //False

var isWorking = true;
var hasProjects = true;
var promotionStatus = isWorking && hasProjects;

console.log(promotionStatus);


